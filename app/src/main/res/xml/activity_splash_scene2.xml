<?xml version="1.0" encoding="utf-8"?>
<MotionScene
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="2500"
        motion:autoTransition="animateToEnd">
        <KeyFrameSet>
            <KeyAttribute
                android:scaleX="1"
                motion:framePosition="0"
                motion:motionTarget="@+id/imageView" />
            <KeyAttribute
                android:scaleY="1"
                motion:framePosition="0"
                motion:motionTarget="@+id/imageView" />
            <KeyAttribute
                android:scaleY="1.1"
                motion:framePosition="10"
                motion:motionTarget="@+id/imageView" />
            <KeyAttribute
                android:scaleX="1.1"
                motion:framePosition="10"
                motion:motionTarget="@+id/imageView" />
            <KeyAttribute
                android:scaleY="1"
                motion:framePosition="20"
                motion:motionTarget="@+id/imageView" />
            <KeyAttribute
                android:scaleX="1"
                motion:framePosition="20"
                motion:motionTarget="@+id/imageView" />
            <KeyAttribute
                android:scaleY="1.1"
                motion:framePosition="30"
                motion:motionTarget="@+id/imageView" />
            <KeyAttribute
                android:scaleX="1.1"
                motion:framePosition="30"
                motion:motionTarget="@+id/imageView" />
            <KeyAttribute
                android:scaleY="1"
                motion:framePosition="40"
                motion:motionTarget="@+id/imageView" />
            <KeyAttribute
                android:scaleX="1"
                motion:framePosition="40"
                motion:motionTarget="@+id/imageView" />
            <KeyAttribute
                motion:motionTarget="@+id/imageView"
                motion:framePosition="100"
                android:scaleY="20" />
            <KeyAttribute
                motion:motionTarget="@+id/imageView"
                motion:framePosition="100"
                android:scaleX="20" />
            <KeyAttribute
                motion:motionTarget="@+id/imageView2"
                motion:framePosition="0"
                android:scaleX="1" />
            <KeyAttribute
                motion:motionTarget="@+id/imageView2"
                motion:framePosition="0"
                android:scaleY="1" />
            <KeyAttribute
                motion:motionTarget="@+id/imageView2"
                motion:framePosition="100"
                android:scaleY="1.2" />
            <KeyAttribute
                motion:motionTarget="@+id/imageView2"
                motion:framePosition="100"
                android:scaleY="1.2" />
            <KeyAttribute
                motion:motionTarget="@+id/imageView2"
                motion:framePosition="100"
                android:scaleX="1.2" />
            <KeyAttribute
                motion:motionTarget="@+id/name"
                motion:framePosition="0"
                android:scaleX="1.2" />
            <KeyAttribute
                motion:motionTarget="@+id/name"
                motion:framePosition="0"
                android:scaleY="1.2" />
            <KeyAttribute
                motion:motionTarget="@+id/name"
                motion:framePosition="100"
                android:scaleX="1.5" />
            <KeyAttribute
                motion:motionTarget="@+id/name"
                motion:framePosition="100"
                android:scaleY="1.5" />
            <KeyAttribute
                motion:motionTarget="@+id/name"
                motion:framePosition="0"
                android:translationY="0dp" />
            <KeyAttribute
                motion:motionTarget="@+id/name"
                motion:framePosition="100"
                android:translationY="45dp" />
        </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>